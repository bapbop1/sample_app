---
import LayoutHeader from '../components/LayoutHeader.astro';
import LayoutFooter from '../components/LayoutFooter.astro';
const { children } = Astro.slots;
const { url } = Astro;
---

<!DOCTYPE HTML>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="robots" content="index, follow">
    <title>【2025年最新】おすすめオンラインカジノランキング - 安全＆高還元率</title>
    <meta name="description" content="2025年最新版！信頼できるオンラインカジノを厳選レビュー。入金不要ボーナス、出金スピード、サポート体制を徹底比較。初心者にもおすすめ！">
    <link rel="canonical" href="https://sample-app-one-xi.vercel.app/" />

    <meta property="og:title" content="【2025年版】オンラインカジノレビュー - 信頼性・ボーナス徹底比較">
    <meta property="og:description" content="今すぐチェック！日本人向けの安全なオンラインカジノを紹介。高還元率・入金不要ボーナス・即時出金。">
    <meta property="og:url" content="https://sample-app-one-xi.vercel.app/">
    <meta property="og:type" content="website">
    <meta property="og:image" content="https://sample-app-one-xi.vercel.app/og-image.jpg">

    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="/assets/css/main.css?v=9" />

    <script src="/assets/js/jquery.min.js" is:inline></script>
    <script src="/assets/js/jquery.dropotron.min.js" is:inline></script>
    <script src="/assets/js/browser.min.js" is:inline></script>
    <script src="/assets/js/breakpoints.min.js" is:inline></script>
    <script src="/assets/js/util.js" is:inline></script>
    <script src="/assets/js/main.js" is:inline></script>
  </head>
  <body class="is-preload homepage">
    <div id="page-wrapper">
      <LayoutHeader currentPath={url.pathname}/>
      <main>
        <slot />
      </main>
      <LayoutFooter />
    </div>
  </body>
</html>
